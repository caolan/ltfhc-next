{
  "schema": {
    "title": "HIV Counseling Child",
    "_id": "hivCounselingChild",
    "type": "object",
    "properties": {
      "child_given_npv_during_labor": {
        "type": "string",
        "enum": [
          "Yes",
          "No",
          "Unknown"
        ]
      },
      "milk_source": {
        "type": "string",
        "enum": [
          "Mother's Milk ONLY",
          "Mother's Milk + Formula",
          "Mother's Milk + Other",
          "Formula ONLY",
          "Formula + Other",
          "Other ONLY"
        ]
      },
      "hiv_diagnosis_dna_pcr": {
        "type": "string",
        "enum": [
          "Yes",
          "No",
          "Unknown"
        ]
      },
      "hiv_diagnosis_antibody": {
        "type": "string",
        "enum": [
          "Yes",
          "No",
          "Unknown"
        ]
      },
      "hiv_diagnosis_date": {
        "type": "string",
        "enum": [
          "Unknown",
          "Less Than 2 Month After Birth",
          "Between 2-18 Months After Birth",
          "More Than 18 Months After Birth"
        ]
      },
      "child_taking_ctx": {
        "type": "string",
        "enum": [
          "Yes",
          "No",
          "Unknown"
        ]
      },
      "child_start_ctx": {
        "type": "string",
        "enum": [
          "Yes",
          "No",
          "Unknown"
        ]
      },
      "other_treatment": {
        "type": "string"
      },
      "note": {
        "type": "string"
      }
    }
  },
  "options": {
    "fields": {
      "child_given_npv_during_labor": {
        "label": "Child Given NPV Syrup in Labor and Delivery Ward?",
        "type": "radio"
      },
      "milk_source": {
        "label": "Child Milk Source:",
        "type": "select"
      },
      "hiv_diagnosis_dna_pcr": {
        "label": "Was Child Diagnosed With HIV Using DNA-PCR(DBS)?",
        "type": "radio"
      },
      "hiv_diagnosis_antibody": {
        "label": "Was Child Diagnosed With HIV Antibody or PCR As A Second Treatment?",
        "type": "radio"
      },
      "hiv_diagnosis_date": {
        "label": "When Did Diagnosis Take Place?",
        "type": "select"
      },
      "child_taking_ctx": {
        "label": "Is Child Taking CTX?",
        "type": "radio"
      },
      "child_start_ctx": {
        "label": "Did Child Start On CTX Between 4 Weeks and 2 Months of Age?",
        "type": "radio"
      },
      "other_treatment": {
        "label": "If Any Treatment Given, Please Describe:",
        "type": "textarea"
      },
      "note": {
        "label": "Other Notes For This Visit:",
        "type": "textarea"
      }
    }
  }
}