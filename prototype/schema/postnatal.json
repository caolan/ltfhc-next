  "options": {
    "fields": {
      "days_post_partum": {
        "label": "Days post-partum",
        "type": "number"
      },
      "completed_4_visits": {
        "label": "Completed all 4 visits?",
        "type": "radio"
      },
      "maternal_diagnoses0": {
        "label": "Maternal diagnoses, check all that apply",
        "type": "checkbox",
        "rightLabel": "severe anemia (Hb<8.5)"
      },
      "maternal_diagnoses1": {
        "type": "checkbox",
        "rightLabel": "mental disorder"
      },
      "maternal_diagnoses2": {
        "type": "checkbox",
        "rightLabel": "gaping or infected perineum"
      },
      "maternal_diagnoses3": {
        "type": "checkbox",
        "rightLabel": "fistula"
      },
      "delivered_outside": {
        "label": "Delivered outside the health center?",
        "type": "radio"
      },
      "family_planning_counseling": {
        "label": "Counseling regarding family planning given?",
        "type": "radio"
      },
      "family_planning_method_after_delivery": {
        "label": "Received a family planning method after delivery?",
        "type": "radio"
      },
      "contraception_within_40_days": {
        "label": "Contraception within 40 days of delivery?",
        "type": "radio"
      },
      "family_planning_method_after_miscarriage": {
        "label": "Received a family planning method after miscarriage?",
        "type": "radio"
      },
      "postnatal_hiv_positive": {
        "label": "Tested positive for HIV postnatally?",
        "type": "radio"
      },
      "milk_type": {
        "label": "How is the child fed?",
        "type": "select"
      },
      "bcg_opv0": {
        "label": "Please check if given to the child",
        "type": "checkbox",
        "rightLabel": "BCG"
      },
      "bcg_opv1": {
        "type": "checkbox",
        "rightLabel": "OPV-0"
      },
      "less_than_2.5kg": {
        "label": "Child born less than 2.5kg?",
        "type": "radio"
      },
      "kangaroo_care": {
        "label": "Kangaroo care given at home?",
        "type": "radio"
      },
      "child_diagnoses0": {
        "label": "Child diagnoses",
        "type": "checkbox",
        "rightLabel": "septicemia"
      },
      "child_diagnoses1": {
        "type": "checkbox",
        "rightLabel": "umbilical cord infection"
      },
      "child_diagnoses2": {
        "type": "checkbox",
        "rightLabel": "severe anemia (Hb<10)"
      },
      "child_diagnoses3": {
        "type": "checkbox",
        "rightLabel": "skin infection"
      },
      "child_diagnoses4": {
        "type": "checkbox",
        "rightLabel": "neonatal death"
      }
    }
  }
