      },
      "possible_hiv": {
        "label": "Is HIV Possible?",
        "type": "radio"
      },
      "hiv_status": {
        "label": "HIV Status",
        "type": "radio"
      },
      "hiv_pcr_test_2mo": {
        "label": "Tested for HIV by PCR within two months from birth?",
        "type": "radio"
      },
      "hiv_pcr_test_18mo": {
        "label": "Tested for HIV by PCR within 6 weeks after stopping breastfeeding or 18 months?",
        "type": "radio"
      },
      "ctx_status": {
        "label": "Started on cotrimoxazole between 1 month to 2 months?",
        "type": "radio"
      },
      "referred_ctc": {
        "label": "Referred CTC?",
        "type": "radio"
      },
      "bed_net_voucher": {
        "label": "Given bed net voucher?",
        "type": "radio"
      }
    }
  }
