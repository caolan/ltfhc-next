{
  "schema": {
    "title": "Well Child Visit",
    "_id": "well_child_visit",
    "type": "object",
    "properties": {
      "height": {
        "type": "number"
      },
      "weight": {
        "type": "number"
      },
      "alben_tablets": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      },
      "milk_source": {
        "type": "string",
        "enum": [
          "Mother's Milk ONLY",
          "Mother's Milk + Formula",
          "Mother's Milk + Other",
          "Formula ONLY",
          "Formula + Other",
          "Other ONLY"
        ]
      },
      "mother_hiv_status": {
        "type": "string",
        "enum": [
          "Positive",
          "Negative",
          "Unknown"
        ]
      },
      "mother_tetanus_delivery": {
        "type": "string",
        "enum": [
          "Yes",
          "No",
          "Unknown"
        ]
      },
      "possible_hiv": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      },
      "hiv_status": {
        "type": "string",
        "enum": [
          "Positive",
          "Negative",
          "Unknown"
        ]
      },
      "hiv_pcr_test_2mo": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      },
      "hiv_pcr_test_18mo": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      },
      "ctx_status": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      },
      "referred_ctc": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      },
      "bed_net_voucher": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      }
    }
  },
  "options": {
    "fields": {
      "height": {
        "label": "Height (cm)",
        "type": "number"
      },
      "weight": {
        "label": "Weight (kg)",
        "type": "number"
      },
      "alben_tablets": {
        "label": "Given albendazole?",
        "type": "radio"
      },
      "milk_source": {
        "label": "Milk Source",
        "type": "select"
      },
      "mother_hiv_status": {
        "label": "Mother's HIV Status",
