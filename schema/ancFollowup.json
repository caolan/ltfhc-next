{
  "schema": {
    "title": "Follow-up Antenatal Visit",
    "_id": "ancFollowup",
    "type": "object",
    "properties": {
      "due_date": {
        "type": "number"
      },
      "number_pregnancy": {
        "type": "number"
      },
      "gestational_age": {
        "type": "number"
      },
      "symptom0": {
        "type": "string"
      },
      "symptom1": {
        "type": "string"
      },
      "symptom2": {
        "type": "string"
      },
      "symptom3": {
        "type": "string"
      },
      "symptom4": {
        "type": "string"
      },
      "symptom5": {
        "type": "string"
      },
      "symptom6": {
        "type": "string"
      },
      "symptom7": {
        "type": "string"
      },
      "symptom8": {
        "type": "string"
      },
      "symptom9": {
        "type": "string"
      },
      "symptom10": {
        "type": "string"
      },
      "symptom11": {
        "type": "string"
      },
      "symptom12": {
        "type": "string"
      },
      "symptom13": {
        "type": "string"
      },
      "other_symptom": {
        "type": "string"
      },
      "patient_weight": {
        "type": "number"
      },
      "fundal_height": {
        "type": "number"
      },
      "cervical_dilation": {
        "type": "number"
      },
      "fetal_station": {
        "type": "string",
        "enum": [
          "Not Examined",
          "High/Unengaged",
          -3,
          -2,
          -1,
          0,
          1,
          2,
          3
        ]
      },
      "hemoglobin": {
        "type": "number"
      },
      "fetal_movement": {
        "type": "string",
        "enum": [
          "Too Early To Be Felt",
          "Felt By Mother",
          "Felt By Mother, Noted to be Abnormal",
          "Not Felt By Mother"
        ]
      },
      "fetal_heart_rate": {
        "type": "string",
        "enum": [
          "Not Checked",
          "Checked, But Unable To Hear",
          "110s",
          "120s",
          "130s",
          "140s",
          "150s",
          "160s",
          "170s",
          "180s"
        ]
      },
      "hiv_status": {
        "type": "string",
        "enum": [
          "Positive",
          "Negative",
          "Unknown"
        ]
      },
      "treatment_given": {
        "type": "string"
      },
      "notes": {
        "type": "string"
      },
      "admission": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      },
      "in_labor": {
        "type": "string",
        "enum": [
          "Yes",
          "No"
        ]
      }
    }
  },
  "options": {
    "fields": {
      "due_date": {
        "label": "Expected Due Date:",
        "type": "number"
      },
      "number_pregnancy": {
        "label": "Pregnancy Number:",
        "type": "number"
      },
      "gestational_age": {
        "label": "Approximate Estiamted Gestational Age (weeks):",
        "type": "number"
      },
      "symptom0": {
        "label": "Symptoms:",
        "type": "checkbox",
        "rightLabel": "None"
      },
      "symptom1": {
        "type": "checkbox",
        "rightLabel": "Abdominal Pain"
      },
      "symptom2": {
        "type": "checkbox",
        "rightLabel": "Vaginal Pain"
      },
      "symptom3": {
        "type": "checkbox",
        "rightLabel": "Vaginal Bleeding"
      },
      "symptom4": {
        "type": "checkbox",
        "rightLabel": "Loss of Fetal Movement"
      },
      "symptom5": {
        "type": "checkbox",
        "rightLabel": "Vaginal Leakage of Fluid"
      },
      "symptom6": {
        "type": "checkbox",
        "rightLabel": "Headache"
      },
      "symptom7": {
        "type": "checkbox",
        "rightLabel": "Blurred Vision"
      },
      "symptom8": {
        "type": "checkbox",
        "rightLabel": "Nausea or Vomiting"
      },
      "symptom9": {
        "type": "checkbox",
        "rightLabel": "Diarrhea"
      },
      "symptom10": {
        "type": "checkbox",
        "rightLabel": "Constipation"
      },
      "symptom11": {
        "type": "checkbox",
        "rightLabel": "Fever"
      },
      "symptom12": {
        "type": "checkbox",
        "rightLabel": "Contractions"
      },
      "symptom13": {
        "type": "checkbox",
        "rightLabel": "Post-Coital Bleeding"
      },
      "other_symptom": {
        "label": "If Other Symptoms, Please Describe:"
      },
      "patient_weight": {
        "label": "Patient Weight in Kg:",
        "type": "number"
      },
      "fundal_height": {
        "label": "Fundal Height (cm):",
        "type": "number"
      },
      "cervical_dilation": {
        "label": "Cervical Dilation (cm):",
        "type": "number"
      },
      "fetal_station": {
        "label": "Fetal Station:",
        "type": "select"
      },
      "hemoglobin": {
        "label": "Hemoglobin (g/dl):",
        "type": "number"
      },
      "fetal_movement": {
        "label": "Fetal Movement:",
        "type": "select"
      },
      "fetal_heart_rate": {
        "label": "Fetal Heart Rate:",
        "type": "select"
      },
      "hiv_status": {
        "label": "HIV Status:",
        "type": "select"
      },
      "treatment_given": {
        "label": "If any treatment given, please describe:",
        "type": "textarea"
      },
      "notes": {
        "label": "Other Application Notes:",
        "type": "textarea"
      },
      "admission": {
        "label": "Admission required today?",
        "type": "radio"
      },
      "in_labor": {
        "label": "Laboring Now?",
        "type": "radio"
      }
    }
  }
}