{
    "fields": {
        "injury_date": {
            "label": "Tarehe ya majeraha",
            "type": "date",
            "dateFormat": "yy-mm-dd",
            "datepicker": {
                "hideIfNoPrevNextType": true,
                "yearRange": "2010:+10",
                "changeMonth": true,
                "changeYear": true
            }
        },
        "injury_type": {
            "label": "Aina ya majeraha",
            "type": "select"
        },
        "injury_other": {
            "label": "Other Injury:"
        },
        "testing_method": {
            "label": "Amepimwa kwa njia zifuatazo",
            "type": "textarea"
        },
        "treatment": {
            "label": "Matibabu",
            "type": "select"
        },
        "treatment_other": {
            "label": "Matibabu nyingine",
            "type": "textarea"
        },
        "admission": {
            "label": "Admission Required?",
            "type": "radio",
            "enum": [
                "Yes",
                "No"
            ],
            "removeDefaultNone": true
        },
        "injury_mortality": {
            "label": "Je majeraha yalisababisha kifo?",
            "type": "radio",
            "enum": [
                "Yes",
                "No"
            ],
            "removeDefaultNone": true
        }
    }
}