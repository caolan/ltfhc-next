{
  "collection": "forms",
  "class": "schema",
  "parent": "visit",
  "_id": "forms/schema/visit/immunization",
  "formname": "Immunization",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Immunization",
  "properties": {
    "dispensed": {
      "title": "Medication Dispensed",
      "type": "string",
      "enum": [
        "A",
        "B",
        "C"
      ]
    },
    "immediateeffects": {
      "title": "Immediate Effects",
      "type": "string",
      "enum": [
        "none",
        "shortness of breath",
        "tongue swelling",
        "rash",
        "fever",
        "swelling at injection site",
        "nausea or vomiting",
        "other"
      ]
    },
    "otherreaction": {
      "title": "If other vaccine reaction, please describe:",
      "type": "string"
    },
    "anaphylaxis": {
      "title": "Anaphylaxis present?",
      "type": "boolean",
      "required": true,
      "default": false
    },
    "counseling": {
      "title": "Describe any counseling given regarding side-effects",
      "type": "string"
    },
    "pregnant": {
      "title": "Pregnant?",
      "type": "boolean",
      "required": true,
      "default": false
    }
  }
}